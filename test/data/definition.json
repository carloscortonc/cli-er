{
  "nms": {
    "kind": "namespace",
    "description": "Description for the namespace",
    "options": {
      "cmd": {
        "kind": "command",
        "type": "string",
        "description": "Description for the command",
        "options": {
          "opt": {
            "type": "number"
          }
        }
      }
    }
  },
  "nmsi": {
    "kind": "namespace",
    "hidden": true,
    "options": {
      "nested-nms": {
        "kind": "namespace",
        "options": {
          "cmd": {
            "kind": "command"
          },
          "nested-nms-o": {
            "kind": "option"
          }
        }
      },
      "nmsi-o": {
        "kind": "option"
      }
    }
  },
  "gcmd": {
    "kind": "command",
    "description": "Description for global command"
  },
  "globalOption": {
    "kind": "option",
    "aliases": ["-g", "global"],
    "default": "globalvalue",
    "description": "Option shared between all commands"
  }
}
